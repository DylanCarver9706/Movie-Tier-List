{
    "name": "movie-tier-list",
    "version": "1.0.0",
    "description": "A movie tier list application with React frontend and Express backend",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
